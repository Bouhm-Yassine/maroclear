{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": ["*"],
      "allow_headers": ["content-type", "authorization"],
      "expose_headers": ["Content-Length"],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS",
        "TRACE"
      ],
      "allow_credentials": true
    }
  },
  "timeout": "300000ms",
  "cache_ttl": "1s",
  "name": "API Gateway",
  "port": 8080,
  "output_encoding": "json",
  "endpoints": [{{ template "Endpoint" .service }}]
}
